   "source": [
    "def p(L, n1=1, d1=3,  n2=1, d2=2):\n",
    "    n = [0] * (L+1) \n",
    "    for d in range(1, L+1):\n",
    "        n[d] += div(n2*d, d2) - div(n1*d, d1) - 1\n",
    "        n[2*d::d] = [k-n[d] for k in n[2*d::d]]\n",
    "    return sum(n)"
   ]
  },
